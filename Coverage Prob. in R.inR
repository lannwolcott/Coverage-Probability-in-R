   "source": [
    "#using the K_A and K_B values to calculate the coverage probability using the pt function\n",
    "\n",
    "coverage_A_basis = pt(-K_A/(sqrt(1+(1/d))),f,lower.tail=FALSE)\n",
    "coverage_B_basis = pt(-K_B/(sqrt(1+(1/d))),f,lower.tail=FALSE)\n",
    "\n",
    "coverage_A_basis\n",
    "coverage_B_basis"
   ]
  },
